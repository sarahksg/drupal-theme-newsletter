{% set item_final_url =  content.field_item_link.0['#url']|render|striptags|trim %}
{% set image_transparent = content.field_image_transparency|render|striptags|trim %}
{# Are there any uploaded images? #}
{% if content.field_item_thumbnail['#items'] %}
    {% set thumbnail_background_color = '#ffffff' %}
  {% set thumbnail_image_url = file_url(content.field_item_thumbnail['#items'].entity.uri.value) %}
  {% set thumbnail_image_title = content.field_item_thumbnail['#items'].alt %}
  {% set image_exists = 'yes' %}
  
{# Are there any links to images? #}
{% elseif content.field_link_to_external_image.0['#url']|length %}
    {% set thumbnail_background_color = '#ffffff' %}
    {% set thumbnail_image_url = content.field_link_to_external_image.0['#url']|render|striptags|trim %}
    {% set thumbnail_image_title = content.field_link_to_external_image.0['#title'] %}
    {% set image_exists = 'yes' %}
    
{# There are no images #}
{% else %}
    {% set thumbnail_background_color = '' %}
{% endif %}
{# Is the image transparent? #}  
{%if (image_exists == 'yes') and (image_transparent == 'yes') %} 
    {% set thumbnail_background_color = '' %}
{% endif %}  
{# Is there custom link text? #} 
{% if content.field_item_link.0['#title']|length %}
    
    {% set link_title = content.field_item_link.0['#title']|render %}
 
{% else %}
    {% set link_title  = 'LEARN MORE' %}
 
{% endif %} 

